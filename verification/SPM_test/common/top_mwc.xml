<?xml version="1.0" encoding="UTF-8"?>
    <!-- no end because the end is inside the following FW image. -->
<HBootImage type="REGULAR" device="SQIROM0" offset="0x00F97000"  has_header="True" has_end="False" >

  <!-- Use the magic cookie for the alternative image. -->
  <Header>
    <Value index="0">0xf3ad9e00</Value>
  </Header>

  <Chunks>

    <!-- following chunk gives global information like name of board config and version of board config -->
    <!-- A simple text chunk is sufficient, but available is only a XIP chunk inside this version of the hboot_image_compiler -->
    <!-- format is
         HWCVxxx.xxx.xxx.xxxx
    	 NAM:xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx  44char reserved for name -->
    <XIP>                                                           
      <File name="@tBoardInfoReadable" load_address="0x10F97048"/>  
    </XIP>                                                          
  
    <Include name="@tBoardconfig" /> 
    
    <!-- Breakpoint: Jump to a "bx lr" instruction in netX 4000 ROM at 0x041120c4 -->
    <!-- increment the target address by one to change to Thumb mode -->
    <Execute>
      <Address>0x041120c5</Address>
    </Execute>


    <!-- fill with dummy data until end of HW data is reached -->
    <!-- check if HW data is not to big -->
    <Skip absolute="0x00FB5FF4" fill="0x40" />	

    <!-- Skip over fdl and FW File Header V3 until the offset 0x00FB6200. -->
    <SkipIncomplete absolute="0x00FB6200" />	

  </Chunks>
</HBootImage>